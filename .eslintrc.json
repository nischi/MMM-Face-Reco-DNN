{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["standard"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": 0,
    "space-before-function-paren": 0,
    "no-undef": 0,
    "semi": 0
  }
}
